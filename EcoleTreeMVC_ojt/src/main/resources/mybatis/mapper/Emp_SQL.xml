<mapper namespace="kr.co.ecoletree.service.emp.mapper.EmpMapper">



	<select id="selectEmpList" parameterType="map" resultType="map">
		select
			a.user_cd
			, a.num
			, a.type
			, a.name
		from TB_TEST a
		where 1=1
	</select>
	
	
	
	<select id="selectEmpListCnt" parameterType="map" resultType="int">
		select
			count(*)
		from TB_TEST a
		where 1=1
	</select>

	
</mapper>